  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@web/test-runner": "^0.17.0",
    "bower": "^1.8.14",
    "coffeelint": "^2.1.0",
    "coffeescript": "^2.7.0",
    "esbuild": "^0.19.0",
    "esbuild-coffeescript": "^2.2.0",
